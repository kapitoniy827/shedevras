// PUT запрос
const putData = {
    "name": "John Doe",
    "age": 30
};

pm.test("Проверка статуса ответа", () => {
    pm.response.to.have.status(200);
});

pm.test("Проверка тела ответа PUT", () => {
    const response = pm.response.json();
    pm.expect(response.data).to.eql(putData);
});
